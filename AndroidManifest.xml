<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.jam.ics"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATIO"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <uses-library android:name="com.google.android.maps"/>
        <activity
            android:name=".CaloriesActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalorieTrackerActivity" android:label="Calorie Tracker Activity"></activity>
        <activity android:name=".Filter" android:label="Filter Activity"></activity>
        <activity android:name=".RestaurantList" android:label="Restaurant List"></activity>
        <activity android:name=".MenuList" android:label="Restaurant Menu"></activity>
        <service android:name=".StepsCounter"></service>
    </application>

</manifest>